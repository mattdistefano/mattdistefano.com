    "postcss-custom-media": "6.0.0",
    "postcss-import": "11.1.0",
    "preact": "8.2.9",
    "preact-async-route": "2.2.1",
    "preact-cli": "2.2.1",
    "preact-cli-plugin-typescript": "0.2.2",
    "preact-cli-postcss": "1.1.1",
    "preact-cli-sw-precache": "1.0.3",
    "preact-markup": "1.6.0",
