{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            "single",
            "jsx-single"
        ],
        "interface-name": [
            "never-prefix"
        ],
        "member-access": [
            "no-public"
        ],
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "no-namespace": [
            false
        ],
        "arrow-parens": false,
        "member-ordering": [
            false
        ],
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}